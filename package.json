{
  "name": "cdp-template",
  "version": "1.0.0",
  "description": "Template Repository with Chrome DevTools Protocol",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p ./tsconfig.json",
    "start": "node dist/index.js",
    "lint": "eslint -c ./.eslintrc.js src",
    "format": "prettier --write ./src && eslint --fix -c ./.eslintrc.js ./src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/progfay/cdp-template.git"
  },
  "keywords": [
    "cdp"
  ],
  "author": "progfay",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/progfay/cdp-template/issues"
  },
  "homepage": "https://github.com/progfay/cdp-template#readme",
  "devDependencies": {
    "@types/node": "14.14.10",
    "@typescript-eslint/eslint-plugin": "4.8.2",
    "@typescript-eslint/parser": "4.8.2",
    "eslint": "7.14.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-config-prettier-standard": "3.0.1",
    "eslint-config-standard": "16.0.2",
    "eslint-config-standard-with-typescript": "19.0.1",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.1.0",
    "prettier": "2.2.1",
    "prettier-config-standard": "1.0.1",
    "ts-loader": "8.0.11",
    "typescript": "4.1.2"
  },
  "dependencies": {
    "chrome-remote-interface": "^0.28.2"
  }
}
